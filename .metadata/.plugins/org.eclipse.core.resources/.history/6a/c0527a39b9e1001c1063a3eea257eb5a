package repositorio;

public class Brinquedo extends Produto {

	String tipo;
	int idade;

	public Brinquedo(int codigo, String descricao, double preco) {
		super(codigo, descricao, preco);
	}

	public String getTipo() {
		return tipo;
	}

	public void setTipo(String tipo) {
		this.tipo = tipo;
	}

	public int getIdade() {
		return idade;
	}

	public void setIdade(int idade) {
		this.idade = idade;
	}

}
